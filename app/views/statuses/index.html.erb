<%= render 'users/admin_sidebar' %>
<h1>Statuses</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @statuses.each do |status| %>
  <tr>
    <td><%= status.name %></td>
    <td><%= link_to 'Show', status %> |
    <%= link_to 'Edit', edit_status_path(status) %> |
    <%= link_to 'Destroy', status, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

