<%= render 'users/admin_sidebar' %>
<h1>Families</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>

<% @families.each do |family| %>
  <tr>
    <td><%= family.name %></td>
    <td><%= link_to 'Show', family %> | 
    <%= link_to 'Edit', edit_family_path(family) %> | 
    <%= link_to 'Destroy', family, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

